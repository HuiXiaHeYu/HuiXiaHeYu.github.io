#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hxhy/Code/Blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules/vuepress/bin/node_modules:/Users/hxhy/Code/Blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules/vuepress/node_modules:/Users/hxhy/Code/Blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules:/Users/hxhy/Code/Blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hxhy/Code/Blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules/vuepress/bin/node_modules:/Users/hxhy/Code/Blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules/vuepress/node_modules:/Users/hxhy/Code/Blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules:/Users/hxhy/Code/Blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules/vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../../../../vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.3.0_typescript@5_c764b9def8c0b92ce60823da07338d3c/node_modules/vuepress/bin/vuepress-vite.js" "$@"
fi
